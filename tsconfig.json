{
  "compilerOptions": {
    "target": "ES2019",
    "module": "NodeNext",
    "jsx": "react-native",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "NodeNext",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": false,
    "rootDir": "src",
    "ignoreDeprecations": "5.0"
  },
  "include": ["src"]
}