{
  "name": "@tecocraft/rn-deeplinking",
  "version": "0.1.2",
  "description": "Manage deep links (Android app links + iOS universal links) in React Native projects with a simple API.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "dev": "tsup src/index.ts --format esm,cjs --watch"
  },
  "keywords": [
    "react-native",
    "deeplinking",
    "universal-links",
    "app-links",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chiragjanjmeratecocraft/RN-Dynamic-Link-SDK.git"
  },
  "author": "tecocraft <nayan.wadekar@tecocraft.com> (https://github.com/nayantecocraft)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chiragjanjmeratecocraft/RN-Dynamic-Link-SDK/issues"
  },
  "homepage": "https://github.com/chiragjanjmeratecocraft/RN-Dynamic-Link-SDK#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/react": "^18.0.0",
    "@types/react-native": "^0.72.0",
    "del-cli": "^5.1.0",
    "eslint": "^9.22.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "react": "19.1.0",
    "react-native": "0.81.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-native": ">=0.64.0"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
